{
  "name": "htmlcss-sveltekit",
  "version": "0.0.3",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "predeploy": "vite build",
    "deploy": "npx gh-pages -d build --dotfiles",
    "release": "commit-and-tag-version",
    "postrelease": "git push --follow-tags origin main"
  },
  "devDependencies": {
    "@dimforge/rapier3d-compat": "^0.10.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.47",
    "@sveltejs/kit": "next",
    "@threlte/core": "^5.0.2",
    "@threlte/extras": "^4.8.0",
    "@threlte/preprocess": "^0.0.2",
    "@threlte/rapier": "^0.5.0",
    "@types/three": "^0.146.0",
    "@yushijinhun/three-minifier-rollup": "^0.4.0",
    "autoprefixer": "^10.4.7",
    "mdsvex": "^0.10.6",
    "postcss": "^8.4.14",
    "postcss-load-config": "^4.0.1",
    "sass": "^1.56.1",
    "svelte": "^3.44.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.7",
    "svelte-sequential-preprocessor": "^0.0.7",
    "tailwindcss": "^3.1.5",
    "three": "^0.146.0",
    "typescript": "^4.7.4",
    "vite": "^3.1.0"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/inter": "^4.5.14",
    "@fontsource/raleway": "^4.5.11",
    "gh-pages": "^4.0.0",
    "mdsvexamples": "^0.3.2",
    "ntl": "^5.1.0"
  }
